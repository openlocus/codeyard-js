{
  "manifest_version": 3,
  "name": "Twitter History",
  "version": "1.0",
  "description": "Shows you the first two tweets from Twitter History",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "history"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://twitter.com/home"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}